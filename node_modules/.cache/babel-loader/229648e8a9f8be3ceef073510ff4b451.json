CAAA,UAAA,GAAA;AAAA,eAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CAAA,MAAA,CAAA;AAAA,OAAA;AACA,aAAA,mBAAA,IAAA,GAAA,CAAA,GAAA,CAAA,EAAA,IAAA,EAAA;AACA,KAfA;AAgBA,IAAA,kBAhBA,gCAgBA;AACA,UAAA,GAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,OAAA,CAAA,UAAA,GAAA;AAAA,eAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CAAA,KAAA,CAAA;AAAA,OAAA;AACA,aAAA,mBAAA,IAAA,GAAA,CAAA,GAAA,CAAA,EAAA,IAAA,EAAA;AACA;AApBA;AAdA,CAAA","sourcesContent":["<template>\n  <div class=\"home\">\n\n  <input type=\"checkbox\" id=\"filterArtists\" name=\"filterArtists\" v-model=\"filterArtists\">\n    <label for=\"filterArtists\">Filter by artist</label>\n\n    <select v-model=\"selectedArtist\">\n      <option\n        v-for=\"artist in noDuplicatesArtists\"\n        :key=\"artist\"\n        :value=\"artist\"\n      >\n        {{ artist }}\n      </option>\n    </select>\n\n    <br />\n\n    <input type=\"checkbox\" id=\"filterVenues\" name=\"filterVenues\" v-model=\"filterVenues\">\n    <label for=\"filterVenues\">Filter by venue</label>\n\n        <select v-model=\"selectedVenue\">\n      <option\n        v-for=\"venue in noDuplicatesVenues\"\n        :key=\"venue\"\n        :value=\"venue\"\n      >\n        {{ venue }}\n      </option>\n    </select>\n\n\n    <SetCard v-for=\"set in filtered\" :key=\"set.videoId\" :set=\"set\" /> <br />\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport SetCard from \"@/components/SetCard.vue\";\n\nimport SetsData from \"../assets/sets.json\";\nexport default {\n  name: \"Home\",\n  components: {\n    SetCard,\n  },\n  data: () => {\n    return {\n      sets: SetsData,\n      selectedArtist: \"\",\n      selectedVenue: \"\",\n      filterArtists: false,\n      filterVenues: false,\n    };\n  },\n  computed: {\n    filtered() {\n      let tempArr = this.sets;\n      if (this.filterArtists) {\n        tempArr = tempArr.filter((set) => set.artist == this.selectedArtist);\n      }\n      if (this.filterVenues) {\n        tempArr = tempArr.filter((set) => set.venue == this.selectedVenue);\n      }\n      return tempArr.sort((a,b)=>{new Date(a.date) - new Date(b.date)});\n    },\n    noDuplicatesArtists(){\n            let arr = [];\n      this.sets.forEach(set => arr.push(set.artist));\n      return [...new Set(arr)].sort();\n    },\n        noDuplicatesVenues(){\n            let arr = [];\n      this.sets.forEach(set => arr.push(set.venue));\n      return [...new Set(arr)].sort();\n    }\n  },\n};\n</script>\n\n<style scoped>\nSetCard {\n  display: block;\n  margin: auto;\n  width: 50%;\n}\n</style>\n"],"sourceRoot":"src/views"}]}