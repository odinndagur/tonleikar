A,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,UAAA,KAAA,GAAA,KAAA,KAAA,GAAA,GAAA,GAAA,aAAA;AACA,SAFA,MAGA;AACA,UAAA,KAAA,GAAA,CAAA,CAAA,IAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,UAAA,KAAA,GAAA,CAAA,CAAA,IAAA,CAAA,KAAA,CAAA,GAAA,CAAA;;AACA,cAAA,CAAA,KAAA,CAAA,CAAA,CAAA,EAAA;AACA,YAAA,KAAA,CAAA,CAAA,CAAA,GAAA,IAAA;AACA,YAAA,KAAA,CAAA,CAAA,CAAA,GAAA,IAAA;AACA,YAAA,KAAA,CAAA,CAAA,CAAA,GAAA,IAAA;AACA,YAAA,KAAA,CAAA,CAAA,CAAA,GAAA,IAAA;AACA;;AACA,UAAA,KAAA,GAAA,KAAA,KAAA,CAAA,CAAA,CAAA,GAAA,GAAA,GAAA,KAAA,CAAA,CAAA,CAAA,GAAA,GAAA,GAAA,KAAA,CAAA,CAAA,CAAA,GAAA,QAAA;AACA,UAAA,KAAA,GAAA,KAAA,KAAA,CAAA,CAAA,CAAA,GAAA,GAAA,GAAA,KAAA,CAAA,CAAA,CAAA,GAAA,GAAA,GAAA,KAAA,CAAA,CAAA,CAAA,GAAA,QAAA;AACA;;AACA,eAAA,IAAA,IAAA,CAAA,KAAA,IAAA,IAAA,IAAA,CAAA,KAAA,CAAA;AACA,OArBA,CAAA;AAsBA,KAjCA;AAkCA,IAAA,mBAlCA,iCAkCA;AACA,UAAA,GAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,OAAA,CAAA,UAAA,GAAA;AAAA,eAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CAAA,MAAA,CAAA;AAAA,OAAA;AACA,aAAA,mBAAA,IAAA,GAAA,CAAA,GAAA,CAAA,EAAA,IAAA,EAAA;AACA,KAtCA;AAuCA,IAAA,kBAvCA,gCAuCA;AACA,UAAA,GAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,OAAA,CAAA,UAAA,GAAA;AAAA,eAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CAAA,KAAA,CAAA;AAAA,OAAA;AACA,aAAA,mBAAA,IAAA,GAAA,CAAA,GAAA,CAAA,EAAA,IAAA,EAAA;AACA;AA3CA;AAdA,CAAA","sourcesContent":["<template>\n  <div class=\"home\">\n    <input\n      type=\"checkbox\"\n      id=\"filterArtists\"\n      name=\"filterArtists\"\n      v-model=\"filterArtists\"\n    />\n    <label for=\"filterArtists\">Filter by artist</label>\n\n    <select v-model=\"selectedArtist\">\n      <option\n        v-for=\"artist in noDuplicatesArtists\"\n        :key=\"artist\"\n        :value=\"artist\"\n      >\n        {{ artist }}\n      </option>\n    </select>\n\n    <br />\n\n    <input\n      type=\"checkbox\"\n      id=\"filterVenues\"\n      name=\"filterVenues\"\n      v-model=\"filterVenues\"\n    />\n    <label for=\"filterVenues\">Filter by venue</label>\n\n    <select v-model=\"selectedVenue\">\n      <option v-for=\"venue in noDuplicatesVenues\" :key=\"venue\" :value=\"venue\">\n        {{ venue }}\n      </option>\n    </select>\n\n    <SetCard v-for=\"set in filtered\" :key=\"set\" :set=\"set\" /> <br />\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport SetCard from \"@/components/SetCard.vue\";\n\nimport SetsData from \"../assets/sets.json\";\nexport default {\n  name: \"Home\",\n  components: {\n    SetCard,\n  },\n  data: () => {\n    return {\n      sets: SetsData,\n      selectedArtist: \"\",\n      selectedVenue: \"\",\n      filterArtists: false,\n      filterVenues: false,\n    };\n  },\n  computed: {\n    filtered() {\n      let tempArr = this.sets;\n      if (this.filterArtists) {\n        tempArr = tempArr.filter((set) => set.artist == this.selectedArtist);\n      }\n      if (this.filterVenues) {\n        tempArr = tempArr.filter((set) => set.venue == this.selectedVenue);\n      }\n\n      // return tempArr.sort();\n      return tempArr.sort((a, b) => {\n        let dateA,dateB;\n        if(a.date.length <= 4){\n          dateA = '' + dateA + '-' + '01-01T00:00';\n        }\n                if(b.date.length <= 4){\n          dateB = '' + dateB + '-' + '01-01T00:00';\n        }\n        else{\n        dateA = a.date.split(\".\");\n        dateB = b.date.split(\".\");\n        if(!dateA[2]){\n          dateA[1] = '01';\n          dateA[2] = '01';\n          dateB[1] = '01';\n          dateB[2] = '01';\n        }\n        dateA = \"\" + dateA[2] + \"-\" + dateA[1] + \"-\" + dateA[0] + \"T00:00\";\n        dateB = \"\" + dateB[2] + \"-\" + dateB[1] + \"-\" + dateB[0] + \"T00:00\";\n        }\n        return new Date(dateB) - new Date(dateA);\n      });\n    },\n    noDuplicatesArtists() {\n      let arr = [];\n      this.sets.forEach((set) => arr.push(set.artist));\n      return [...new Set(arr)].sort();\n    },\n    noDuplicatesVenues() {\n      let arr = [];\n      this.sets.forEach((set) => arr.push(set.venue));\n      return [...new Set(arr)].sort();\n    },\n    \n  },\n};\n</script>\n\n<style scoped>\nSetCard {\n  display: block;\n  margin: auto;\n  width: 50%;\n}\n</style>\n"],"sourceRoot":"src/views"}]}